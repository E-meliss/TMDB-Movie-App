n `/tv/${this.cardData.id}`;\n      }\n    },\n    getPosterUrl(path) {\n      return `https://image.tmdb.org/t/p/w500${path}`;\n    }\n  }\n};\n</script>\n\n<style>\n.category-card {\n  background-color: #fff;\n  border-radius: 10px;\n  overflow: hidden;\n  transition: transform 0.2s;\n  position: relative;\n  z-index: 1;\n  width: 250px !important;\n}\n\n.category-card:hover {\n  transform: scale(1.05);\n}\n\n.category-card img {\n  width: 100%;\n  height: auto;\n}\n\n.category-card h2 {\n  margin: 10px 0;\n}\n\n\n.card-link {\n  text-decoration: none;\n  color: inherit;\n}\n\n.poster {\n  width: 100%;\n  height: auto;\n}\n\n.card-content {\n  padding: 10px;\n}\n\n.card-content h3 {\n  margin: 0;\n}\n\n.card-content p {\n  font-size: 0.9em;\n  color: #666;\n}\n\n.category-list {\n    display: grid;\n    grid-template-columns: repeat(3, 1fr);\n    margin-top: 100px;\n    margin-left: 100px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}