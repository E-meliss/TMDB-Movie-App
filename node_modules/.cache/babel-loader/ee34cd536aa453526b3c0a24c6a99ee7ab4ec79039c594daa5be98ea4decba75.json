"grid grid-cols-1 sm:grid-cols-2 md:grid-cols-3 lg:grid-cols-5 gap-4\">\n      <div v-for=\"movie in movies\" :key=\"movie.id\" class=\"movie-card\">\n        <img :src=\"'https://image.tmdb.org/t/p/w500' + movie.poster_path\" :alt=\"movie.title\" class=\"w-full rounded-lg shadow-md\">\n        <div class=\"p-4\">\n          <h2 class=\"font-bold text-xl mb-2 text-black\">{{ movie.title }}</h2>\n          <p class=\"text-gray-900\">{{ formatDate(movie.release_date) }}</p>\n          <p class=\"text-gray-900\">Rating: {{ movie.vote_average }}</p>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"PopularMovies\",\n  data() {\n    return {\n      movies: [],\n    };\n  },\n  mounted() {\n    fetch(\n      'https://api.themoviedb.org/3/movie/popular?api_key=348088421ad3fb3a9d6e56bb6a9a8f80&language=en-US&page=1'\n    )\n    .then((response) => response.json())\n    .then((data) => (this.movies = data.results))\n    .catch((error) => console.log(error));\n  },\n  methods: {\n    formatDate(date) {\n      const options = { year: 'numeric', month: 'short', day: 'numeric' };\n      return new Date(date).toLocaleDateString(undefined, options);\n    }\n  }\n};\n</script>\n\n<style scoped>\n.container {\n  padding: 20px;\n  position: relative;\n  z-index: 1;\n}\n\n.grid {\n  display: grid;\n  gap: 16px;\n}\n\n.movie-card {\n  background-color: #fff;\n  border-radius: 10px;\n  overflow: hidden;\n  transition: transform 0.2s;\n  position: relative;\n  z-index: 1;\n}\n\n.movie-card:hover {\n  transform: scale(1.05);\n}\n\n.movie-card img {\n  width: 100%;\n  height: auto;\n}\n\n.movie-card h2 {\n  margin: 10px 0;\n}\n\n.text-black {\n  color: black;\n}\n\n.text-gray-900 {\n  color: #1a1a1a;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}